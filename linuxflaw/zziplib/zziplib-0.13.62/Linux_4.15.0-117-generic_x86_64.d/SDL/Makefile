	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	echo " ( cd '$(DESTDIR)$(zzipsdldir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(zzipsdldir)" && rm -f $$files
